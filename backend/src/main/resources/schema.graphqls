type ComicBookDTO {
    title: String
    number: String
    writers: [PersonDTO]!
}

type PersonDTO {
    firstName: String
    lastName: String
}

type ComicBookQuery {
  allComicBooks: [ComicBookDTO]
}

type ComicBookMutation {
    createComicBook(title: String!, number: String!): ComicBookDTO
}

schema {
  query: ComicBookQuery
  mutation: ComicBookMutation
}
